# Admin Configuration
# Password for admin login (optional - can use OAuth instead)
NUXT_ADMIN_PASSWORD=admin
# Email address to assign admin role (optional)
NUXT_ADMIN_EMAIL=your-email@example.com

# OAuth - Google
# Get credentials from: https://console.cloud.google.com/apis/credentials
NUXT_OAUTH_GOOGLE_CLIENT_ID=your_google_client_id
NUXT_OAUTH_GOOGLE_CLIENT_SECRET=your_google_client_secret

# OAuth - Apple
# Get credentials from: https://developer.apple.com/account/resources/identifiers/list/serviceId
NUXT_OAUTH_APPLE_CLIENT_ID=your_apple_client_id
NUXT_OAUTH_APPLE_TEAM_ID=your_apple_team_id
NUXT_OAUTH_APPLE_KEY_ID=your_apple_key_id
NUXT_OAUTH_APPLE_PRIVATE_KEY_PATH=./AuthKey_XXXXXXXXXX.p8

# Session secret for encryption
NUXT_SESSION_PASSWORD=change-me-to-a-random-string-of-at-least-32-characters
